CREATE TABLE BLACKUSERS(USER VARCHAR(50),BLACKUSER VARCHAR(50))
CREATE TABLE WHITEUSERS(USER VARCHAR(50),WHITEUSER VARCHAR(50))

CREATE TABLE DNDVALUES(USER VARCHAR(50) PRIMARY KEY,MON_STRT VARCHAR(10),MON_END VARCHAR(10),TUE_STRT VARCHAR(10),TUE_END VARCHAR(10),
WED_STRT VARCHAR(10),WED_END VARCHAR(10),THU_STRT VARCHAR(10),THU_END VARCHAR(10),
FRI_STRT VARCHAR(10),FRI_END VARCHAR(10),SAT_STRT VARCHAR(10),SAT_END VARCHAR(10),
SUN_STRT VARCHAR(10),SUN_END VARCHAR(10))