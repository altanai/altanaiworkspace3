CREATE TABLE BLACKUSERS(USER VARCHAR(50),BLACKUSER VARCHAR(50))
CREATE TABLE WHITEUSERS(USER VARCHAR(50),WHITEUSER VARCHAR(50))

CREATE TABLE DNDVALUES(USER VARCHAR(50) PRIMARY KEY,MON_STRT VARCHAR(15),MON_END VARCHAR(15),TUE_STRT VARCHAR(15),TUE_END VARCHAR(15),
WED_STRT VARCHAR(15),WED_END VARCHAR(15),THU_STRT VARCHAR(15),THU_END VARCHAR(15),
FRI_STRT VARCHAR(15),FRI_END VARCHAR(15),SAT_STRT VARCHAR(15),SAT_END VARCHAR(15),
SUN_STRT VARCHAR(15),SUN_END VARCHAR(15))

DROP TABLE DNDVALUES;